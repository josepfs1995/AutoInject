<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Isac Fuentes</Authors>
    <Company>Isac Fuentes</Company>
    <Product>AutoInject</Product>
    <Description>This project is used to automatically generate your depencendy injections for your Web NET Core.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/josepfs1995/AutoInject</RepositoryUrl>
    <PackageReleaseNotes>This project is used to automatically generate your depencendy injections for your Web NET Core.</PackageReleaseNotes>
    <PackageId>autoinject-jf</PackageId>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
